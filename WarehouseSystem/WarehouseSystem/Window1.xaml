				Source="img\stock.png"
				Height="158"
				Width="162"
				Grid.Column="0"
				Grid.Row="0"
				HorizontalAlignment="Left"
				VerticalAlignment="Top"
				Margin="116.5,180,0,0" />
			<Label
				Background="Transparent"
				FontWeight="Light"
				Foreground="White"
				FontSize="13"
				Content="There are two types of companies: those that "
				Width="261"
				Height="29.999999999999943"
				Grid.Column="0"
				Grid.Row="0"
				HorizontalAlignment="Left"
				VerticalAlignment="Top"
				Margin="63.5,363.5,0,0" />
			<Label
				Background="Transparent"
				FontWeight="Light"
				Foreground="White"
				FontSize="13"
				Width="261"
				Height="29.999999999999943"
				Content="have been hacked, and those that will be."
				Grid.Column="0"
				Grid.Row="0"
				HorizontalAlignment="Left"
				VerticalAlignment="Top"
				Margin="69.5,386.5,0,0" />
			<Grid
				Height="30"
				Width="299.99999999999977"
				Background="#FFFF5722"
				x:Name="createAcount_Button"
				Grid.Column="0"
				Grid.Row="0"
				HorizontalAlignment="Right"
				VerticalAlignment="Bottom"
				Margin="0,0,-346.5,113"
				MouseLeftButtonDown="createAcount_Button_MouseLeftButtonDown"
				MouseEnter="createAcount_Button_MouseEnter"
				MouseLeave="createAcount_Button_MouseLeave">
