				MouseLeftButtonDown="signIn_Button_MouseLeftButtonDown"
				MouseEnter="signIn_Button_MouseEnter"
				MouseLeave="signIn_Button_MouseLeave">
				<Label
					Foreground="White"
					FontSize="15"
					FontWeight="SemiBold"
					x:Name="label1_Copy"
					Content="SIGN IN"
					Height="30"
					Width="65.5"
					Grid.Column="0"
					Grid.Row="0"
					HorizontalAlignment="Right"
					VerticalAlignment="Bottom"
					Margin="0,0,116.75,0" />
