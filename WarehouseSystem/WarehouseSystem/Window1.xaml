<?xml version="1.0" encoding="utf-8"?>
<Window
	x:Class="WarehouseSystem.Window1" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	Title="WarehouseSystem"
	ResizeMode="NoResize"
	Height="650"
	WindowStartupLocation="CenterScreen"
	WindowStyle="SingleBorderWindow"
	Width="800">
	<Grid
		Background="#FF263238">
		<Grid
			Margin="441,202,0,0"
			VerticalAlignment="Top"
			HorizontalAlignment="Left"
			Grid.Row="0"
			Grid.Column="0"
			Height="227"
			Width="300">
			<Label
				Foreground="White"
				Height="32"
				Width="141"
				Content="Username"
				Grid.Column="0"
				Grid.Row="0"
				HorizontalAlignment="Left"
				VerticalAlignment="Top"
				Margin="1.49999999999989,5.00000000000011,0,0"
				FontSize="13"
				FontWeight="Light" />
			<TextBox
				Height="30"
				Width="300"
				FontSize="13"
				Foreground="White"
				VerticalContentAlignment="Center"
				Grid.Column="0"
				Grid.Row="0"
				HorizontalAlignment="Left"
				VerticalAlignment="Top"
				Margin="0.5,37.0000000000001,0,0"
				BorderThickness="0,0,0,1"
				FontWeight="Light"
				Background="#FF263238"
				BorderBrush="#FF0277BD"
				x:Name="username_Textbox" />
			<Label
				Foreground="White"
				Height="32"
				Width="141"
				Content="Password"
				Grid.Column="0"
				Grid.Row="0"
				HorizontalAlignment="Left"
				VerticalAlignment="Top"
				Margin="1.49999999999989,75.0000000000001,0,0"
				FontSize="13"
				FontWeight="Light" />
